{
  "listname" : "iPad",
  "consolename" : "iPad",

  "outputs" : [
    { "extension" : "mp4",
      "suffix" : "iPad",
      "faststart" : true,
      "streams" : [
          { "type" : "video",
            "codec" : "libx264",
            "mode" : "crf",
            "quality" : 18,
            "bitrate" : "8M",
            "buffer" : "10M",
            "profile" : "high",
            "level" : "4.2",
            "pixfmt" : "yuv420p",
            "filters" : [
              { "filter" : "scale", "mode" : 1, "width" : 1920, "height" : 1080, "dar" : "16/9" }
            ]
          },

          { "type" : "audio",
            "codec" : "libfdk_aac",
            "bitrate" : "160k",
            "flags" : [
              { "key" : "-ac", "val" : "2" }
            ]
          }
      ]
    }
  ]
}
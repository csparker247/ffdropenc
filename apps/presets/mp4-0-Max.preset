{
  "listname" : "MP4 - H.264/AAC [Max Quality]",
  "consolename" : "MP4",

  "outputs" : [
    { "extension" : "mp4",
      "suffix" : "H264",
      "faststart" : true,
      "streams" : [
        { "type" : "video",
          "codec" : "libx264",
          "mode" : "crf",
          "quality" : 18,
          "profile" : "high",
          "level" : "4.2",
          "pixfmt" : "yuv420p",
          "filters" : [
            { "filter" : "scale", "mode" : 0 }
          ],
          "flags" : [
            { "key" : "-preset", "val" : "veryslow" }
          ]
        },

        { "type" : "audio",
          "codec" : "aac",
          "bitrate" : "320k"
        }
      ]
    }
  ]
}
{
  "listname" : "MP4 - H.264/AAC [Square Pixels]",
  "consolename" : "MP4",

  "outputs" : [
    { "extension" : "mp4",
      "suffix" : "H264",
      "faststart" : true,
      "streams" : [
          { "type" : "video",
            "codec" : "libx264",
            "mode" : "crf",
            "quality" : 18,
            "bitrate" : "15M",
            "buffer" : "12M",
            "profile" : "high",
            "level" : "4.2",
            "pixfmt" : "yuv420p",
            "filters" : [
              { "filter" : "scale", "mode" : 2 }
            ]
          },

          { "type" : "audio",
            "codec" : "aac",
            "bitrate" : "320k"
          }
      ]
    }
  ]
}
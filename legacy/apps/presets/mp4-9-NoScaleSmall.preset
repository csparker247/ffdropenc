{
  "listname" : "MP4 - H.264/AAC [Very Slow]",
  "consolename" : "MP4",

  "outputs" : [
    { "extension" : "mp4",
      "suffix" : "SmallH264",
      "faststart" : true,
      "streams" : [
          { "type" : "video",
            "codec" : "libx264",
            "mode" : "crf",
            "quality" : 18,
            "bitrate" : "7M",
            "buffer" : "8M",
            "profile" : "high",
            "level" : "4.2",
            "pixfmt" : "yuv420p",
            "flags" : [
              { "key" : "-preset", "val" : "veryslow" }
            ]
          },

          { "type" : "audio",
            "codec" : "aac",
            "bitrate" : "320k"
          }
      ]
    }
  ]
}
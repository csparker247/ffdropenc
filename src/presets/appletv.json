{ 
  "preset": {
    "optname": "AppleTV",
    "outputs":
        [
          {"extension": "mp4",
          "suffix": "AppleTV",
          
          "video": {
            "codec": "libx264",
            "mode": "crf",
            "quality": 18,
            "bitrate": "8M",
            "buffer": "10M",
            "profile": "high",
            "level": "4.0",
            "pixfmt": "yuv420p",
            "faststart": true,
            "filters": 
            [
              {"filter": "scale", "mode": 1, "w": 1920, "h": 1080, "dar": "16/9"}
            ]
          },
          
          "audio": 
          { "codec": "libfdk_aac",
            "bitrate": "320k"
          }
          
          }
        ]
  }
}
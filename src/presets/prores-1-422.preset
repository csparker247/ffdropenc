# ProRes 422 Preset

listname = "ProRes 422";
consolename = "ProRes 422";

outputs = ( 
  { extension = "mov";
    suffix = "ProRes"; 
    faststart = false;  
    streams = (
        { type = "video";
          codec = "prores_ks";
          pixfmt = "yuv422p10le";
          bitrate = "IGNORE";
          flags = (
            { flag = "-qscale:v 10" },
            { flag = "-vendor ap10" }
          );
        },

        { type = "audio";
          codec = "pcm_s16le";
          bitrate = "IGNORE"
          flags = (
            { flag = "-filter_complex asetnsamples=n=16384:p=0" },
            { flag = "-ar 48k" }
          );
        }
    );
  }
);